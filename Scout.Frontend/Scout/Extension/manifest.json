{
  "manifest_version": 3,
  "name": "Scout Accountability",
  "version": "1.0",
  "description": "Sends accountability alerts when visiting blocked websites.",
  "permissions": [ "tabs", "storage", "activeTab", "scripting" ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/scout.png",
    "48": "icons/scout.png",
    "128": "icons/scout.png"
  }
}
